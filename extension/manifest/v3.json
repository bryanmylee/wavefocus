{
	"manifest_version": 3,
	"name": "Wave",
	"description": "A simple and beautiful pomodoro app to keep you focused",
	"version": "0.0.1",
	"permissions": ["alarms", "notifications", "storage", "gcm"],
	"background": {
		"service_worker": "./background.js"
	},
	"action": {
		"default_title": "Wave",
		"default_popup": "index.html",
		"default_icon": {
			"32": "assets/toolbar-icon-32.png"
		}
	},
	"icons": {
		"128": "assets/icon-128.png"
	}
}
